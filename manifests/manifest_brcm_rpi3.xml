<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="github" fetch="https://github.com/"/>

    <!-- Raspberry Pi 3 -->
    <project path="device/brcm/rpi3" name="lineage-rpi/android_device_brcm_rpi3" remote="github" />
    <project path="kernel/brcm/rpi3" name="lineage-rpi/android_kernel_brcm_rpi3" remote="github" />
    <project path="vendor/brcm" name="lineage-rpi/proprietary_vendor_brcm" remote="github" />

    <!-- LineageOS additions -->
    <project path="external/i2c-tools" name="lineage-rpi/android_external_i2c-tools" remote="github" />

    <!-- LineageOS replacements -->
    <remove-project name="platform/external/swiftshader"/>
    <project path="external/swiftshader" name="lineage-rpi/android_external_swiftshader" remote="github" />
</manifest>
