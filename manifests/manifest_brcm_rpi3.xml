<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="lineage" fetch="https://github.com/lineage-rpi"/>

    <!-- Raspberry Pi 3 -->
    <project path="device/brcm/rpi3" name="android_device_brcm_rpi3" remote="lineage" />
    <project path="kernel/brcm/rpi3" name="android_kernel_brcm_rpi3" remote="lineage" />
    <project path="vendor/brcm" name="proprietary_vendor_brcm" remote="lineage" />

    <!-- LineageOS additions -->
    <project path="external/i2c-tools" name="android_external_i2c-tools" remote="lineage" />

    <!-- LineageOS replacements -->
    <remove-project name="platform/external/swiftshader"/>
    <project path="external/swiftshader" name="android_external_swiftshader" remote="lineage" />
</manifest>
